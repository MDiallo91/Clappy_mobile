{
  "cli": {
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk"
      }
    },
    "development": {
      "developmentClient": true,
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "env": {
        "EXPO_NO_LEGACY_BUNDLER": "1"
      },
      "cache": {
        "disabled": false
      },
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease"
      }
    }
  }
}
