{
  "expo": {
    "name": "clappymobile",
    "slug": "clappyappymobile",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/Clappy_logo.png",
    "scheme": "clappy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "jsEngine": "hermes",
      "package": "com.cissefode.clappyco",
      "versionCode": 9,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Clappy_logo.png",
        "backgroundColor": "#E6F4FE"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDfnHdioRjLABbJUfebL6uPQvpn0adIPuo"
        }
      },
      "permissions": []
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cissefode.clappy",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Clappy_logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "29c58cfc-b99e-480b-9a4e-3cae68ef27eb"
      }
    },
    "owner": "cissefode"
  }
}
