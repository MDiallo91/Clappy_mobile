{
  "expo": {
    "name": "clappy",
    "slug": "clappy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Clappy_logo.jpg",
    "scheme": "clappy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.expo.clappy"
    },
    "android": {
      "package": "dev.expo.clappy",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundColor": "#E6F4FE"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB1xMP9XpXq7YQwK8nV7mL6zZcD8rS9tUvWxY"
        }
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Clappy_logo.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "613d785e-c49f-4d56-854c-93c11d8ce0aa"
      }
    },
    "owner": "madiallo"
